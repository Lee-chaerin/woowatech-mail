#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/55437/OneDrive/바탕 화면/공부/우테코 8기/woowatech-mail/backend/node_modules/.pnpm/openai@6.8.1/node_modules/openai/bin/node_modules:/mnt/c/Users/55437/OneDrive/바탕 화면/공부/우테코 8기/woowatech-mail/backend/node_modules/.pnpm/openai@6.8.1/node_modules/openai/node_modules:/mnt/c/Users/55437/OneDrive/바탕 화면/공부/우테코 8기/woowatech-mail/backend/node_modules/.pnpm/openai@6.8.1/node_modules:/mnt/c/Users/55437/OneDrive/바탕 화면/공부/우테코 8기/woowatech-mail/backend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/55437/OneDrive/바탕 화면/공부/우테코 8기/woowatech-mail/backend/node_modules/.pnpm/openai@6.8.1/node_modules/openai/bin/node_modules:/mnt/c/Users/55437/OneDrive/바탕 화면/공부/우테코 8기/woowatech-mail/backend/node_modules/.pnpm/openai@6.8.1/node_modules/openai/node_modules:/mnt/c/Users/55437/OneDrive/바탕 화면/공부/우테코 8기/woowatech-mail/backend/node_modules/.pnpm/openai@6.8.1/node_modules:/mnt/c/Users/55437/OneDrive/바탕 화면/공부/우테코 8기/woowatech-mail/backend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/cli" "$@"
else
  exec node  "$basedir/../../bin/cli" "$@"
fi
